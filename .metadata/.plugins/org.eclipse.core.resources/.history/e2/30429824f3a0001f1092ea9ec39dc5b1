package Array;

public class BasicArray {
	public static void main(String[] args) {
	
	int[]arr=new int[5];
	arr[0]=74;
	arr[1]=90;
	arr[2]=56;
	arr[3]=23;
	arr[4]=65;
	
	for (int i=0;i<arr.length;i++) {
		System.out.println(arr[i]);
		
	}
	System.out.println("by inhance for loops=======");
	int[]arr1= {23,54,65,76,23,45};
	
	for(int h:arr1){
		System.out.println();
		}
	int arr2[];
	}
	
	
}
