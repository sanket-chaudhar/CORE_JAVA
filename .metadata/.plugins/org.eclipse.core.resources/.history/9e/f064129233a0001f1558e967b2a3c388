package GeterSeter;

public class Mydata {
private int date;
private String month;
private int year;


public void setdate(int day) {
	date=day;
}
public int getdate() {
	return date;
	
	
}
public void setmonth(String month) {
  this.month=month;
}
public String getmonth() {
	return month;
}


public void setyear(int year) {
	this.year=year;
}
public int getyear() {
	return year;
}

}
